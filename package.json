{
  "name": "dns-resolver-api",
  "version": "1.0.0",
  "description": "Dns Resolver API",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:staging": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:unit": "jest -i --colors --verbose --detectOpenHandles --testPathPattern=src/tests/units",
    "test:integration": "jest -i --colors --verbose --detectOpenHandles --testPathPattern=src/tests/integrations",
    "test:load": "artillery run src/tests/loads/index.yml --quiet --output artillery_report.json",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@faker-js/faker": "^8.0.2",
    "@hokify/agenda": "^6.3.0",
    "@sendgrid/mail": "^8.1.3",
    "axios": "^1.7.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cache-manager": "^6.1.3",
    "celery-node": "^0.5.9",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.13",
    "dns": "^0.2.2",
    "dns2": "^2.1.0",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.8.1",
    "formidable": "^2.1.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "jspdf": "^2.5.1",
    "moment": "^2.29.4",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.4",
    "p-limit": "^3.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^5.3.0",
    "stripe": "^16.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "twilio": "^5.2.0",
    "validator": "^13.9.0",
    "winston": "^3.10.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/dns2": "^2.0.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "artillery": "^2.0.0-35",
    "coveralls": "^3.1.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6",
    "yaml-js": "^0.3.1"
  }
}